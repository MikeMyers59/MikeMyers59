<SCRIPT text="text\vbscipt" language="VBScript">
dim strHelpDocPath  :  strHelpDocPath = "MikesMenuHelp.docx"

Sub Load()
  ResizeWindow 1040,800 
  intPosX = CInt( ( window.screen.width  - document.body.offsetWidth  ) / 2 )
  intPosY = CInt( ( window.screen.height - document.body.offsetHeight ) / 2 )
  If intPosX < 0 Then intPosX = 0
  If intPosY < 0 Then intPosY = 0
  RelocateWindow intPosX, intPosY
  startclock()  ' call to a javascript subroutine for the countdown timer!
End Sub

</SCRIPT>

</HEAD>
 
